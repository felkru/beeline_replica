{
  "manifest_version": 2,
  "name": "Beeline Replica & Bionic Reader",
  "version": "1.0",
  "description": "A replica of Beeline Reader with Bionic Reading capabilities.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "Beeline Replica Settings"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
